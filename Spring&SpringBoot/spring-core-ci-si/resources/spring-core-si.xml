<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">
       
	<bean id="product" class="com.institute.bean.Product">
		<property name="id" value="111"></property>
		<property name="name" value="Smartphone"></property>
		<property name="price" value="25000.50"></property>
		
		<property name="comment">
			<list>
				<value>This is very good product</value>
				<value>This product use for basic use</value>
				<value>High Price</value>
			</list>
		</property>
		<property name="model">
			<set>
				<value>SmartPhone-3GB-RAM</value>
				<value>SmartPhone-6GB-RAM</value>
				<value>SmartPhone-8GB-RAM</value>
			</set>	
		</property>
		<property name="ratings">
			<map>
				<entry key="5.5" value="20"></entry>
				<entry key="4.2" value="60"></entry>
				<entry key="4" value="37"></entry>
			</map>
		</property>
	</bean>																																												    
</beans>